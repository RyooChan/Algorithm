import java.util.*;

class Solution {
    public int solution(int[] citations) {
        int answer = 0;
        
        Arrays.sort(citations);
        
        int cnt=0;
        for(int i=citations.length-1; i>=0; i--){
            System.out.println(citations[i]);
            if(cnt>=citations[i]) break;
            cnt++;
        }
        return cnt;
    }
}
